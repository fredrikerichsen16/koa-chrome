{
    "name": "Hallo",
    "version": "1.0.0",
    "manifest_version": 2,
    "chrome_url_overrides": {
        "newtab": "/views/chrome.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "http://localhost/"
    ],
    "icons": {
        "16": "favicon.ico"
    }
}